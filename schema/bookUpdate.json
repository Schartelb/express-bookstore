{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "isbn"
    ],
    "properties": {
        "isbn": {
            "type": "integer",
            "default": 0,
            "title": "The isbn Schema",
            "examples": [
                1681606489483
            ],
            "maxLength": 13,
            "minLength": 10
        },
        "amazon_url": {
            "type": "string",
            "default": "",
            "title": "The amazon_url Schema",
            "examples": [
                "https://www.amazon.com/Fairy-Tale-Stephen-King-ebook/dp/B09QXZB6SK/ref=lp_24437326011_1_2"
            ]
        },
        "author": {
            "type": "string",
            "default": "",
            "title": "The author Schema",
            "examples": [
                "Stephen King"
            ]
        },
        "language": {
            "type": "string",
            "default": "English",
            "title": "The language Schema",
            "examples": [
                "English"
            ]
        },
        "pages": {
            "type": "integer",
            "default": 0,
            "title": "The pages Schema",
            "examples": [
                486
            ]
        },
        "publisher": {
            "type": "string",
            "default": "",
            "title": "The publisher Schema",
            "examples": [
                "Simon and Schuster"
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Fairy Tale"
            ]
        },
        "year": {
            "type": "integer",
            "default": 0,
            "title": "The year Schema",
            "examples": [
                2022
            ],
            "maximum": 2022
        }
    },
    "examples": [
        {
            "isbn": 1681606489483,
            "amazon_url": "https://www.amazon.com/Fairy-Tale-Stephen-King-ebook/dp/B09QXZB6SK/ref=lp_24437326011_1_2",
            "author": "Stephen King",
            "language": "English",
            "pages": 486,
            "publisher": "Simon and Schuster",
            "title": "Fairy Tale",
            "year": 2022
        }
    ]
}